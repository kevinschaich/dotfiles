				"win": "ctrl+shift+m",
				"linux": "ctrl+shift+m",
				"key": "ctrl+shift+m",
				"command": "markdown.showPreviewToSide"
			},
			{
				"mac": "ctrl+alt+cmd+l",
				"win": "alt+ctrl+r",
				"linux": "alt+ctrl+r",
				"key": "alt+ctrl+r",
				"command": "workbench.action.reloadWindow"
			},
			{
				"mac": "ctrl+shift+o",
				"key": "none",
				"command": "editor.action.openLink"
			},
			{
				"mac": "alt+cmd+i",
				"win": "ctrl+alt+i",
				"linux": "ctrl+alt+i",
				"key": "ctrl+alt+i",
				"command": "workbench.action.toggleDevTools"
			},
			{
				"mac": "alt+shift+s",
				"win": "alt+shift+s",
				"linux": "alt+shift+s",
				"key": "alt+shift+s",
				"command": "editor.action.showSnippets"
			},
			{
				"win": "ctrl+shift+o",
				"linux": "ctrl+shift+o",
				"key": "ctrl+shift+o",
				"command": "workbench.action.files.openFolder"
			},
			{
				"mac": "cmd+shift+o",
				"key": "none",
				"command": "workbench.action.files.openFileFolder"
			},
			{
				"mac": "ctrl+m",
				"win": "ctrl+m",
				"linux": "ctrl+m",
				"key": "ctrl+m",
