				"mac": "cmd+k cmd+1",
				"win": "ctrl+k ctrl+1",
				"linux": "ctrl+k ctrl+1",
				"key": "ctrl+k ctrl+1",
				"command": "editor.foldLevel1",
				"when": "editorTextFocus"
			},
			{
				"mac": "cmd+k cmd+2",
				"win": "ctrl+k ctrl+2",
				"linux": "ctrl+k ctrl+2",
				"key": "ctrl+k ctrl+2",
				"command": "editor.foldLevel2",
				"when": "editorTextFocus"
			},
