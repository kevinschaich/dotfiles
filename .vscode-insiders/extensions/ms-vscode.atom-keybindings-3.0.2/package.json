			{
				"mac": "ctrl+cmd+f",
				"win": "f11",
				"linux": "f11",
				"key": "f11",
				"command": "workbench.action.toggleFullScreen"
			},
			{
				"mac": "cmd+r",
				"win": "ctrl+r",
				"linux": "ctrl+r",
				"key": "ctrl+r",
				"command": "workbench.action.gotoSymbol"
			},
			{
				"mac": "alt+cmd+[",
				"win": "ctrl+alt+/",
				"linux": "ctrl+alt+/",
				"key": "ctrl+alt+/",
				"command": "editor.fold",
				"when": "editorTextFocus"
			},
			{
				"mac": "alt+cmd+]",
				"win": "ctrl+alt+/",
				"linux": "ctrl+alt+/",
				"key": "ctrl+alt+/",
				"command": "editor.unfold",
				"when": "editorTextFocus"
			},
			{
				"mac": "alt+shift+cmd+[",
				"win": "ctrl+alt+[",
				"linux": "ctrl+alt+[",
				"key": "ctrl+alt+[",
				"command": "editor.foldAll",
				"when": "editorTextFocus"
			},
			{
				"mac": "alt+shift+cmd+]",
				"win": "ctrl+alt+]",
				"linux": "ctrl+alt+]",
				"key": "ctrl+alt+]",
				"command": "editor.unfoldAll",
				"when": "editorTextFocus"
			},
			{
				"mac": "cmd+shift+7",
				"key": "cmd+k cmd+c",
